<resources>

    <style name="tencent_tls_ui_theme" parent="android:Theme.Holo.Light.NoActionBar"></style>
    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">-->
        <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    <!--</style>-->

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="spilt_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#EDEDED</item>
    </style>

    <style name="main_tab_bottom">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@drawable/selector_tab_text_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="cb_time">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/selector_circle_text</item>
        <item name="android:background">@drawable/selector_circle</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="multiple_switch">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:text">""</item>
        <item name="android:textOn">""</item>
        <item name="android:textOff">""</item>
        <item name="android:focusable">false</item>
        <item name="android:button">@null</item>
        <item name="android:checked">false</item>
        <item name="android:background">@drawable/selector_multiple_switch</item>
    </style>
    <style name="multiple_switch2">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:text">""</item>
        <item name="android:textOn">""</item>
        <item name="android:textOff">""</item>
        <item name="android:focusable">false</item>
        <item name="android:button">@null</item>
        <item name="android:checked">false</item>
        <item name="android:background">@drawable/selector_multiple_switch</item>
    </style>
    <style name="multiple_switch_text">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="scene_switch">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:text">""</item>
        <item name="android:textOn">""</item>
        <item name="android:textOff">""</item>
        <item name="android:focusable">false</item>
        <item name="android:button">@null</item>
        <item name="android:checked">false</item>
    </style>
    <!--<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

    <!--<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />-->

    <!--<style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

    <!--<style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />-->

    <style name="RadiobuttonStyle">
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:button">@null</item>
    </style>
    <style name="popupWindowAnimation_bottom_to_top" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/bottom_to_top</item>
        <item name="android:windowExitAnimation">@anim/top_to_bottom</item>
    </style>

    <!--极光推送相关-->
    <style name="push_style">
        <item name="android:hint">@string/style_hint</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_alias">
        <item name="android:hint">@string/alias_hint</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_tag">
        <item name="android:hint">@string/tag_hint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="CustomDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

</resources>
